[core]
	autocrlf = input
	safecrlf = true
[push]
	default = simple
[branch]
	autosetuprebase = always
[merge "ours"]
	driver = true
[merge]
  keepBackup = false
  tool = p4merge
[mergetool "p4merge"]
  cmd = /Applications/p4merge.app/Contents/MacOS/p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
  keepTemporaries = false
  trustExitCode = false
  keepBackup = false
