[alias]
  refresh = "!git fetch upstream && git merge upstream/master"
  put = push origin head
	co = checkout
	st = status -sb
	ci = commit
  pr = pull --rebase -p
	puhs = push
  graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr by %an)%Creset' --abbrev-commit --date=relative
	tfpull = tf pull --rebase
	tfpush = tf checkin --deep --no-lock
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	my-bad = blame
	ds = diff --staged
[color]
	ui = auto
